											"direction": "Input"
										},
										{
											"name": "Trigger_Type",
											"type": "String",
											"value": {
												"value": "@pipeline().TriggerType",
												"type": "Expression"
											},
											"direction": "Input"
										},
										{
											"name": "Trigger_time",
											"type": "Datetime",
											"value": {
												"value": "@pipeline().TriggerTime",
												"type": "Expression"
											},
											"direction": "Input"
										},
										{
											"name": "Status",
											"type": "String",
											"value": {
												"value": "R",
												"type": "Expression"
											},
											"direction": "Input"
										}
									],
									"type": "Query",
									"text": "INSERT INTO ETL_LOAD_HIST\n(DF_Name, Pipeline_Name, Pipeline_runID, Trigger_Name, Trigger_Type, Trigger_time, Status)\nVALUES (@DF_Name, @Pipeline_Name, @Pipeline_runID, @Trigger_Name, @Trigger_Type, @Trigger_time, @Status)\n"
								}
							],
							"scriptBlockExecutionTimeout": "02:00:00"
						}
					},
					{
						"name": "Script2",
						"type": "Script",
						"dependsOn": [
							{
								"activity": "Script1",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
