												"value": "@pipeline().Pipeline",
												"type": "Expression"
											},
											"direction": "Input"
										},
										{
											"name": "Pipeline_runID",
											"type": "String",
											"value": {
												"value": "@pipeline().RunId",
												"type": "Expression"
											},
											"direction": "Input"
										},
										{
											"name": "Trigger_Name",
											"type": "String",
											"value": {
												"value": "@pipeline().TriggerName",
												"type": "Expression"
