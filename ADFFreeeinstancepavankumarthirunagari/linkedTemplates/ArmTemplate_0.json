											"value": {
												"value": "@pipeline().RunId",
												"type": "Expression"
											},
											"direction": "Input"
										}
									],
									"type": "Query",
									"text": "UPDATE ETL_LOAD_HIST SET\nStatus = 'S'\nWHERE Pipeline_runID =@pipelineRunID\nand [Status] = 'R'"
